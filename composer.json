{
    "name": "eleven59/vimeo-connector",
    "description": "Helper class to connect to Vimeo and download video information based on video ID",
    "license": "MIT",
    "authors": [
        {
            "name": "Nik Linders",
            "email": "info@eleven59.nl",
            "homepage": "https://eleven59.nl"
        }
    ],
    "homepage": "https://github.com/eleven59/vimeo-connector",
    "keywords": [
        "Backpack Addon", "VimeoConnector"
    ],
    "require": {
        "illuminate/support": "~7|~8",
        "backpack/crud": "^4.1.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Eleven59\\VimeoConnector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eleven59\\VimeoConnector\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eleven59\\VimeoConnector\\AddonServiceProvider"
            ]
        }
    }
}
